{
    "ComponentName": "QualMainTable",
    "BlockType": "table",
    "TableOptions": {
        "ItemsSource": "{Session.Drivers}",
        "Limit": "{g6}",
        "IndexStart": "{SessisonStartPos, Converter=NumberSubtract, Parameter=1}",
        "RowSpacing": 30,
        "HeaderTemplate": {
            "BlockType": "text",
            "MarginBottom": 108,
            "Style": "TableSessionHeader"
        },
        "Columns": [
            {
                "Header": "",
                "Template": {
                    "BlockType": "component",
                    "Component": "MainColumn",
                    "ComponentOptions": {
                        "Vars": {
                            "style": "P1Default",
                            "fg": "{SessionFG}",
                            "font": "{FontNameItalic}",
                            "w": "45",
                            "pad": "12,0",
                            "source": "{Item.Position}"
                        }
                    }
                }
            },
            {
                "Header": "",
                "Template": {
                    "BlockType": "component",
                    "Component": "V1DriverLine",
                    "ComponentOptions": {
                        "Vars": {
                            "style1": "FirstNameDefault",
                            "style2": "SecondNameDefault"
                        }
                    }
                }
            },
            {
                "Header": "",
                "Template": {
                    "BlockType": "component",
                    "Component": "V1TeamLine",
                    "ComponentOptions": {
                        "Vars": {
                            "bg": "00000000",
                            "teamstyle": "TeamDefault"
                        }
                    }
                }
            },
            {
                "Header": "[TIME]",
                "Template": {
                    "BlockType": "component",
                    "Component": "MainColumn",
                    "ComponentOptions": {
                        "Vars": {
                            "style": "P1Default",
                            "fg": "{SessionFG}",
                            "font": "{FontNameItalic}",
                            "w": "150",
                            "pad": "20,0",
                            "source": "{Item.AbsoluteTime}"
                        }
                    },
                    "Triggers": [
                        {
                            "Condition": "{Item.IsRaceFastestLap}",
                            "ComponentVar": "fg",
                            "Value": "{SessionFastestLapFG}"
                        },
                        {
                            "Condition": "{Item.FastestLapTime, Converter=EmptyObjectToTrue}",
                            "ComponentVar": "source",
                            "Value": "[No Time]"
                        }
                    ]
                }
            },
            {
                "Header": "[GAP]",
                "Template": {
                    "RenderIf": "{QualTime}",
                    "BlockType": "component",
                    "Component": "MainColumn",
                    "ComponentOptions": {
                        "Vars": {
                            "style": "P1Default",
                            "fg": "{SessionFG}",
                            "font": "{FontNameItalic}",
                            "w": "150",
                            "pad": "20,0",
                            "source": "{Item.Time}"
                        }
                    },
                    "Triggers": [
                        {
                            "Condition": "{Item.IsRaceFastestLap}",
                            "ComponentVar": "fg",
                            "Value": "{SessionFastestLapFG}"
                        },
                        {
                            "Condition": "{Item.FastestLapTime, Converter=EmptyObjectToTrue}",
                            "ComponentVar": "source",
                            "Value": "[No Time]"
                        }
                    ]
                }
            },
            {
                "Header": "[TYRE]",
                "Template": {
                    "RenderIf": "{QualTyre}",
                    "BlockType": "dock",
                    "Width": "114",
                    "Padding": "0,-5",
                    "VerticalAlignment": "Center",
                    "HorizontalAlignment": "Center",
                    "Items": [
                        {
                            "BlockType": "image",
                            "Source": "tyres/tyres_{Item.FastestLapTyres}.png",
                            "VerticalAlignment": "Center",
                            "HorizontalAlignment": "Center",
                            "Height": 50
                        }
                    ]
                }
            },
            {
                "Header": "[S1]",
                "Template": {
                    "RenderIf": "{QualSector}",
                    "BlockType": "component",
                    "Component": "MainColumn",
                    "ComponentOptions": {
                        "Vars": {
                            "style": "P1Default",
                            "fg": "{SessionFG}",
                            "font": "{FontNameItalic}",
                            "w": "150",
                            "pad": "5,0",
                            "source": "{Item.FastestLapSector1}"
                        }
                    },
                    "Triggers": [
                        {
                            "Condition": "{Item.IsOverallFastestSector1}",
                            "ComponentVar": "fg",
                            "Value": "{SessionFastestLapFG}"
                        },
                        {
                            "Condition": "{Item.FastestLapSector1, Converter=EmptyObjectToTrue}",
                            "ComponentVar": "source",
                            "Value": "- - . - - -"
                        }
                    ]
                }
            },
            {
                "Header": "[S2]",
                "Template": {
                    "RenderIf": "{QualSector}",
                    "BlockType": "component",
                    "Component": "MainColumn",
                    "ComponentOptions": {
                        "Vars": {
                            "style": "P1Default",
                            "fg": "{SessionFG}",
                            "font": "{FontNameItalic}",
                            "w": "150",
                            "pad": "5,0",
                            "source": "{Item.FastestLapSector2}"
                        }
                    },
                    "Triggers": [
                        {
                            "Condition": "{Item.IsOverallFastestSector2}",
                            "ComponentVar": "fg",
                            "Value": "{SessionFastestLapFG}"
                        },
                        {
                            "Condition": "{Item.FastestLapSector2, Converter=EmptyObjectToTrue}",
                            "ComponentVar": "source",
                            "Value": "- - . - - -"
                        }
                    ]
                }
            },
            {
                "Header": "[S3]",
                "Template": {
                    "RenderIf": "{QualSector}",
                    "BlockType": "component",
                    "Component": "MainColumn",
                    "ComponentOptions": {
                        "Vars": {
                            "style": "P1Default",
                            "fg": "{SessionFG}",
                            "font": "{FontNameItalic}",
                            "w": "150",
                            "pad": "5,0",
                            "source": "{Item.FastestLapSector3}"
                        }
                    },
                    "Triggers": [
                        {
                            "Condition": "{Item.IsOverallFastestSector3}",
                            "ComponentVar": "fg",
                            "Value": "{SessionFastestLapFG}"
                        },
                        {
                            "Condition": "{Item.FastestLapSector2, Converter=EmptyObjectToTrue}",
                            "ComponentVar": "source",
                            "Value": "- - . - - -"
                        }
                    ]
                }
            },
            {
                "Header": "[LAPS]",
                "Template": {
                    "RenderIf": "{QualLaps}",
                    "BlockType": "component",
                    "Component": "MainColumn",
                    "ComponentOptions": {
                        "Vars": {
                            "style": "P1Default",
                            "fg": "{SessionFG}",
                            "font": "{FontNameItalic}",
                            "w": "50",
                            "pad": "40,0",
                            "source": "{Item.LapsCount}"
                        }
                    }
                }
            },
            {
                "Header": "[POINTS]",
                "Template": {
                    "RenderIf": "{QualPoints}",
                    "BlockType": "component",
                    "Component": "MainColumn",
                    "ComponentOptions": {
                        "Vars": {
                            "style": "P1Default",
                            "fg": "{PointsFG}",
                            "font": "{FontNameBold}",
                            "w": "50",
                            "pad": "50,0",
                            "source": "{Item.DriverPointsString}"
                        }
                    },
                    "Triggers": [
                        {
                            "Condition": "{Item.DriverPointsString, Converter=EmptyObjectToFalse}",
                            "ComponentVar": "source",
                            "Value": "+{Item.DriverPointsString}"
                        },
                        {
                            "Condition": "{Item.DriverPointsString, Converter=EmptyObjectToTrue}",
                            "ComponentVar": "source",
                            "Value": "0"
                        }
                    ]
                }
            }
        ]
    }
}