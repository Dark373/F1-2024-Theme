{
    "BlockRoot": {
        "BlockType": "stack",
        "Background": "{Season.MainLeagueCategory.Color}",
        "Items": [
            {
                "BlockType": "image",
                "Source": "bg/bg.{g1}.png",
                "Triggers": [
                    {
                        "Condition": "{g2}",
                        "Property": "Source",
                        "Value": "bg/bg.{g1}.plain.png"
                    },
                    {
                        "Condition": "{Season.SeasonOverallNumberEvents, Converter=NumberGreater, Parameter=13}",
                        "ConditionAnd": "{Season.SeasonOverallNumberEvents, Converter=NumberLess, Parameter=35}",
                        "Setters": [
                            {
                                "Property": "WidthPercent",
                                "Value": 100
                            },
                            {
                                "Property": "HeightPercent",
                                "Value": 100
                            }
                        ]
                    }
                ]
            }
        ]
    }
}